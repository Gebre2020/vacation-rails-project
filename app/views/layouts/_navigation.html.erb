  <div>    
      <% if !logged_in? %><br/>
        <%= link_to "Sign Up", signup_path %> |
        <%= link_to "Log In", login_path %> |
        <%= button_to "Sign in with Google", '/auth/google_oauth2'%>
      <% else %>
        <%= link_to "Add New Trip", new_trip_path %> |
        <%= link_to "All Trip", trips_path %> |
        <%= link_to "Add New Location", new_location_path %> |
        <%= link_to "All Location", locations_path %> |
        <%= button_to "Logout", logout_path, method:"delete" %>
      <% end %>
    </div> 